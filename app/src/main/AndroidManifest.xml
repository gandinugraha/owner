<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mrrobot.owner">

    
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AddNewActivity" />
        <activity android:name=".activity.InformationActivity" />
        <activity android:name=".activity.ServiceActivity" />
        <activity android:name=".activity.StylishActivity" />
        <activity android:name=".activity.MasterPiecesActivity" />
        <activity android:name=".activity.ReviewActivity" />
        <activity android:name=".activity.PromosActivity" />
        <activity android:name=".activity.SubscribesActivity" />
        <activity android:name=".activity.DetailInformationActivity" />
        <activity android:name=".activity.HistoryActivity" />
        <activity android:name=".activity.HistoryDetailActivity" />
        <activity android:name=".activity.UserActivity" />
        <activity android:name=".activity.PaymentActivity"></activity>
    </application>

</manifest>